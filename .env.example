POSTGRES_DB=safe_tx
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

DATABASE_URL=postgresql://postgres:postgres@db:5432/safe_tx
L2_RPC_URL=https://proxy.prividium.dev/rpc
PRIVIDIUM_RPC_URL=https://proxy.prividium.dev/rpc
CHAIN_ID=7777
DEPLOYER_PRIVATE_KEY=0x...

# RPC auth mode. For deployer init with siwe, this uses regular Prividium SIWE + deployer wallet.
TENANT_AUTH_MODE=siwe
PRIVIDIUM_PERMISSIONS_API_BASE_URL=https://permissions-api.prividium.dev
TENANT_AUDIENCE=

# Runtime tenant SIWE (for privileged receipt/indexing RPC, optional)
TENANT_SIWE_BASE_URL=https://permissions-api.prividium.dev
TENANT_PRIVATE_KEY=0x...

# API/runtime settings
L1_RPC_URL=https://l1.example.rpc
SERVICE_PRIVATE_KEY=0x...
L1_RELAYER_PRIVATE_KEY=0x...

# Optional: pre-existing MultiSend deployment
MULTISEND_ADDRESS=

# Optional runtime overrides.
L1_NULLIFIER_ADDRESS=
L1_BRIDGEHUB_ADDRESS=
L1_ASSET_ROUTER_ADDRESS=

ALLOW_ADVANCED_CALLDATA=false
ALLOW_DELEGATECALL=false
ALLOW_CUSTOM_TARGETS=false

# Optional init extension for private contracts (JSON array)
PRIVATE_CONTRACT_DEPLOYMENTS=

# UI profile vars
VITE_PRIVIDIUM_CLIENT_ID=
VITE_AUTH_BASE_URL=
VITE_REDIRECT_URL=http://localhost:5173/auth/callback
